<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="dev.ln13.cse360project.frontend.UserType">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="appNameText" styleClass="title"/>
    <Label fx:id="userTypeText"/>
    <Button text="I'm a Patient" onAction="#patientLogin"/>
    <Button text="I'm a Provider" onAction="#providerLogin"/>
</VBox>
